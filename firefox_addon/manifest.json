{

  "manifest_version": 2,
  "name": "Subte",
  "version": "0.1",

  "description": "Estado del subte",

  "icons": {
    "48": "icons/linea_C.jpg"
  },
  
  "browser_action": {
    "default_icon": "icons/lineaC.jpg",
    "default_title": "Subte",
    "default_popup": "popup/subte.html"
  },

  "permissions": ["http://*/*", "https://*/*"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["subte.js"]
    }
  ]

}


