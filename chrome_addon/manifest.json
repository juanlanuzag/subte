{

  "manifest_version": 2,
  "name": "Subte",
  "version": "0.25",

  "description": "Estado del subte",

  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },

  "browser_action": {
    "default_icon": "icons/logo.png",
    "default_title": "Subte",
    "default_popup": "popup/subte.html"
  },
  "permissions": ["http://www.metrovias.com.ar/","alarms"],
  "background": {
    "scripts": ["popup/jquery-3.1.1.min.js", "background.js"]
  }
}


